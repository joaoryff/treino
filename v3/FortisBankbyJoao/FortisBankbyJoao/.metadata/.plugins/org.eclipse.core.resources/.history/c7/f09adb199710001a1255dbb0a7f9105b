package bus;

public class Saving extends Account{
	private int s_number;
	private int t_number;
	private int s_balance;
	private char s_fee;
	public int getS_number() {
		return s_number;
	}
	public void setS_number(int s_number) {
		this.s_number = s_number;
	}
	public int getT_number() {
		return t_number;
	}
	public void setT_number(int t_number) {
		this.t_number = t_number;
	}
	public int getS_balance() {
		return s_balance;
	}
	public void setS_balance(int s_balance) {
		this.s_balance = s_balance;
	}
	public char getS_fee() {
		return s_fee;
	}
	public void setS_fee(char s_fee) {
		this.s_fee = s_fee;
	}
	public Saving(int ac_number, int pin, Date ac_opendate, float balance,
			enumAccType ac_type, int ac_id, int s_number, int t_number,
			int s_balance, char s_fee) {
		super(ac_number, pin, ac_opendate, balance, ac_type, ac_id);
		this.s_number = s_number;
		this.t_number = t_number;
		this.s_balance = s_balance;
		this.s_fee = s_fee;
	}
	public Saving(int ac_number, int pin, Date ac_opendate, float balance,
			enumAccType ac_type, int ac_id) {
		super(ac_number, pin, ac_opendate, balance, ac_type, ac_id);
		this.s_number = s_number;
		this.t_number = t_number;
		this.s_balance = s_balance;
		this.s_fee = s_fee;
	}
	public Saving() {
		super();
		this.s_number = 0;
		this.t_number = 0;
		this.s_balance = 0;
		this.s_fee = ' ';
	}
	
	public String toString() {
		return "Saving [s_number=" + s_number + ", t_number=" + t_number
				+ ", s_balance=" + s_balance + ", s_fee=" + s_fee + "]";
	}
	

}
